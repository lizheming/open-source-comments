      "patch": "@@ -0,0 +1,65 @@\n+import cn from \"classnames\";\n+import React, {\n+  ChangeEvent,\n+  EventHandler,\n+  FunctionComponent,\n+  KeyboardEvent,\n+} from \"react\";\n+\n+import { withStyles } from \"coral-ui/hocs\";\n+\n+import styles from \"./TextArea.css\";\n+\n+export interface TextAreaProps {\n+  name?: string;\n+  value?: string;\n+  placeholder?: string;\n+  color?: \"regular\" | \"streamBlue\" | \"error\";\n+\n+  className?: string;\n+  classes: typeof styles;\n+\n+  disabled?: boolean;\n+\n+  onChange?: EventHandler<ChangeEvent<HTMLTextAreaElement>>;\n+  onKeyPress?: EventHandler<KeyboardEvent>;\n+  onKeyDown?: EventHandler<KeyboardEvent>;\n+}\n+\n+const TextArea: FunctionComponent<TextAreaProps> = ({\n+  name,\n+  value,\n+  placeholder,\n+  color,\n+  className,\n+  classes,\n+  ...rest\n+}) => {\n+  const rootClassName = cn(classes.root, className);\n+\n+  const inputClassName = cn(classes.input, {\n+    [classes.colorStreamBlue]: color === \"streamBlue\",\n+    [classes.colorRegular]: color === \"regular\",\n+    [classes.colorError]: color === \"error\",\n+  });\n+\n+  return (\n+    <div className={rootClassName}>\n+      <textarea\n+        className={inputClassName}\n+        name={name}\n+        value={value}\n+        placeholder={placeholder}\n+        {...rest}\n+      ></textarea>\n+    </div>\n+  );\n+};\n+\n+TextArea.defaultProps = {\n+  color: \"regular\",\n+  placeholder: \"\",\n+} as Partial<TextAreaProps>;\n+\n+const enhanced = withStyles(styles)(TextArea);\n+export default enhanced;"
